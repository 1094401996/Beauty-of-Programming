package twodot2;
/*
 * 
 */
public class FactorialProblem {
	
	
	
	
	public static int problem1Method1(int N){
		int num = 0;
		for(int i = 5 ; i <= N; i++){
			int j  = i;
			while(j % 5 ==0 ){
				num ++;
				j /= 5;
			}
		}
		
		return num;
	}	
	
	
	public static int problem1Method2(int N){
		int num = 0;
		
		while(N != 0){
			num += N/5;
			N/=5;
			
		}
		
		return num;
	}
}
